<mvc:View
    controllerName="com.anjali.layout.controller.View5"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
    xmlns:f="sap.ui.layout.form"
    xmlns:core="sap.ui.core"
    xmlns:ux="sap.uxap"
>
    <Page
        id="page"
        title="Object Page layout"
        showNavButton="true"
        navButtonPress="onBack"
    >
        <content>
            <ux:ObjectPageLayout>
                <ux:headerTitle>
                    <ux:ObjectPageHeader objectTitle="Jai Ram" />
                </ux:headerTitle>

                <ux:headerContent>
                    <ObjectAttribute
                        title="Emp id"
                        text="162987"
                    />
                    <ObjectAttribute
                        title="Designation"
                        text="Jr.employee"
                    />
                    <ObjectAttribute
                        title="Salary"
                        text="12000"
                    />
                </ux:headerContent>

                <ux:sections />
                <ux:ObjectPageSection title="Basic details">
                    <ux:ObjectPageSubSection>
                        <f:SimpleForm editable="false">
                            <f:content>
                                <Label text="Employee id" />
                                <Text text="162004" />
                                <Label text="Employee Name" />
                                <Text text="Jai ram" />
                                <Label text="Employee Designation" />
                                <Text text="Sr.employee" />
                                <Label text="Employee Skill" />
                                <Text text="ABAP,Ui5" />
                            </f:content>
                        </f:SimpleForm>
                    </ux:ObjectPageSubSection>
                </ux:ObjectPageSection>

                <ux:ObjectPageSection title="Contact details">
                    <ux:ObjectPageSubSection>
                        <f:SimpleForm editable="false">
                            <f:content />
                            <Label text="Employee MobileNo" />
                            <Text text="746237462" />
                            <Label text="Employee Email" />
                            <Text text="ABAP@gmail.com" />
                        </f:SimpleForm>
                    </ux:ObjectPageSubSection>
                </ux:ObjectPageSection>
            </ux:ObjectPageLayout>
        </content>
    </Page>
</mvc:View>
